<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuMantenimientoClientesEmpresaController">
    <children>
        <VBox layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="26.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="26.0" AnchorPane.topAnchor="14.0">
            <children>
                <Button fx:id="buttomRegresarMenuPrincipal" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#clickButtomRegresarMenuPrincipal" prefHeight="18.0" prefWidth="114.0" style="-fx-background-color: yellow;" text="Regresar al menu principal">
                    <font>
                        <Font name="System Bold" size="8.0" />
                    </font>
                </Button>
                <Label text="Mantenimiento de Clientes de Tipo Empresa">
                    <font>
                        <Font name="System Bold" size="24.0" />
                    </font>
                </Label>
                <Label text="Razon Social " />
                <TextField fx:id="textFiedlRazonSocial" />
                <Label text="RUC" />
                <TextField fx:id="textFiedlRUC" />
                <Label text="Direccion" />
                <TextField fx:id="textFiedlDireccion" />
                <Button fx:id="buttomAgregarClienteTipoEmpresa" mnemonicParsing="false" onAction="#clickButtomAgregarClienteEmpresa" style="-fx-background-color: black;" text="Agregar Cliente" textFill="WHITE">
                    <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="buttomEditarClienteTipoEmpresa" mnemonicParsing="false" onAction="#clickButtomEditarClienteEmpresa" text="Editar Cliente Existente" translateX="110.0" translateY="-35.0" />
                <Button fx:id="buttomEliminarClienteTipoPersona" mnemonicParsing="false" onAction="#clickButtomEliminarClienteEmpresa" style="-fx-background-color: #ef4040;" text="Eliminar " textFill="WHITE" translateX="260.0" translateY="-60.0" />

                <Label text="Lista de Clientes de Tipo Empresa" translateY="-25.0">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <TableView fx:id="tablaClientesTipoEmpresa" prefHeight="118.0" prefWidth="560.0" translateY="-25.0">
                    <columns>
                        <TableColumn fx:id="columnaRazonSocial" prefWidth="190.0" text="Razon Social" />
                        <TableColumn fx:id="columnaRUC" prefWidth="112.0" text="RUC" />
                        <TableColumn fx:id="columnaDireccion" prefWidth="155.0" text="Direccion" />
                    </columns>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>
