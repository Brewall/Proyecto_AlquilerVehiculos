<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuPrincipalController">
    <children>
        <VBox alignment="CENTER" layoutX="243.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0" spacing="30.0"
              AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0"
              AnchorPane.topAnchor="20.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <children>
                <Label alignment="TOP_CENTER" contentDisplay="TOP" text="Panel de Control - AutoRent"
                       textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="24.0"/>
                    </font>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets bottom="20.0"/>
                    </padding>
                </Label>
                <GridPane alignment="CENTER" hgap="10.0" vgap="10.0">
                    <columnConstraints>
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="125.0"/>
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="125.0"/>
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="125.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="100.0" valignment="CENTER" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="100.0" valignment="CENTER" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <VBox fx:id="idmodreservas" alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
                              style="-fx-border-color: black;">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" text="Modulo Reservas">
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                    <font>
                                        <Font name="System Bold" size="12.0"/>
                                    </font>
                                </Label>
                                <Label alignment="CENTER" text="Gestionar reserva de vehiculos " textFill="#00000080">
                                    <opaqueInsets>
                                        <Insets/>
                                    </opaqueInsets>
                                    <padding>
                                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                                    </padding>
                                    <VBox.margin>
                                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font name="System Italic" size="10.0"/>
                                    </font>
                                </Label>
                                <Button fx:id="buttomAccederReservas" alignment="BOTTOM_CENTER" contentDisplay="BOTTOM"
                                        mnemonicParsing="false" onAction="#clickButtomAccederReservas"
                                        style="-fx-background-color: black;" text="Acceder" textFill="WHITE">
                                    <padding>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </padding>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>


                        <VBox fx:id="idmoddevoluciones" alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
                              style="-fx-border-color: black;" GridPane.columnIndex="1">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" text="Modulo Devoluciones">
                                    <VBox.margin>
                                        <Insets bottom="2.0" left="10.0" right="10.0" top="2.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font name="System Bold" size="12.0"/>
                                    </font>
                                </Label>
                                <Label alignment="CENTER" prefHeight="19.0" prefWidth="150.0"
                                       text="Procesar devoluciones de vehiculos" textFill="#00000080">
                                    <padding>
                                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                                    </padding>
                                    <VBox.margin>
                                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font size="10.0"/>
                                    </font>
                                </Label>
                                <Button fx:id="buttomAccederDevoluciones" alignment="BOTTOM_CENTER"
                                        contentDisplay="BOTTOM" mnemonicParsing="false"
                                        onAction="#clickButtomAccederDevoluciones" style="-fx-background-color: black;"
                                        text="Acceder" textFill="WHITE">
                                    <padding>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </padding>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>


                        <VBox fx:id="idmodvehiculos" alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
                              style="-fx-border-color: black;" GridPane.columnIndex="2">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="50.0" prefWidth="144.0"
                                       text="Modulo Vehiculos" textAlignment="CENTER" wrapText="true">
                                    <VBox.margin>
                                        <Insets bottom="2.0" left="10.0" right="10.0" top="2.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font name="System Bold" size="12.0"/>
                                    </font>
                                </Label>
                                <Label alignment="CENTER" prefHeight="22.0" prefWidth="150.0"
                                       text="Gestionar el mantenimiento de vehiculos" textFill="#00000080">
                                    <padding>
                                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                                    </padding>
                                    <VBox.margin>
                                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font name="System Italic" size="10.0"/>
                                    </font>
                                </Label>
                                <Button fx:id="buttomAccederMantenimientoVehiculo" alignment="BOTTOM_CENTER"
                                        contentDisplay="BOTTOM" mnemonicParsing="false"
                                        onAction="#clickButtomAccederMantenimientoVehiculo"
                                        style="-fx-background-color: black;" text="Acceder" textFill="WHITE">
                                    <padding>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </padding>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>


                        <VBox fx:id="idmodclientes" alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
                              style="-fx-border-color: black;" GridPane.rowIndex="1">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="43.0" prefWidth="145.0"
                                       text="Modulo Clientes" textAlignment="CENTER" wrapText="true">
                                    <VBox.margin>
                                        <Insets bottom="2.0" left="10.0" right="10.0" top="2.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font name="System Bold" size="12.0"/>
                                    </font>
                                </Label>
                                <Label alignment="CENTER" prefWidth="150.0" text="Gestionar informacion de los clientes"
                                       textFill="#00000080">
                                    <padding>
                                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                                    </padding>
                                    <font>
                                        <Font name="System Italic" size="10.0"/>
                                    </font>
                                </Label>
                                <Button fx:id="buttomAccederMantenimientoClientes" alignment="BOTTOM_CENTER"
                                        contentDisplay="BOTTOM" mnemonicParsing="false"
                                        onAction="#clickButtomAccederMantenimientoClientes"
                                        style="-fx-background-color: black;" text="Acceder" textFill="WHITE">
                                    <padding>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </padding>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>


                        <VBox fx:id="idmodalquileres" alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
                              style="-fx-border-color: black;" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="139.0"
                                       text="Modulo  Reporte" wrapText="true">
                                    <VBox.margin>
                                        <Insets bottom="2.0" left="10.0" right="10.0" top="2.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font name="System Bold" size="12.0"/>
                                    </font>
                                </Label>
                                <Label alignment="CENTER" prefWidth="150.0" text="Ver historial y estado de alquileres"
                                       textFill="#00000080">
                                    <padding>
                                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                                    </padding>
                                    <font>
                                        <Font name="System Italic" size="10.0"/>
                                    </font>
                                </Label>
                                <Button fx:id="buttomAccederConsultaAlquiler" alignment="BOTTOM_CENTER"
                                        contentDisplay="BOTTOM" mnemonicParsing="false"
                                        onAction="#clickButtomAccederConsultaAlquiler"
                                        style="-fx-background-color: black;" text="Acceder" textFill="WHITE">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>
                    </children>
                </GridPane>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </VBox>
    </children>
</AnchorPane>
